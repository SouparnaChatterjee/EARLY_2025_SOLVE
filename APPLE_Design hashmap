class MyHashMap {
private:
  static const int size=1000001;
    int mp[size];    
public:
    MyHashMap() {
    std::fill(mp, mp+size, -1);
    }
    
    void put(int key, int value) {
    mp[key] =value;   
    }
    
    int get(int key) {
    return mp[key];

    }
    
    void remove(int key) {
    mp[key]=-1;    
    }
};
